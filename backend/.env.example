# Database Configuration
MONGODB_URI=mongodb://localhost:27017/ai-tutor-app

# Google API Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Server Configuration
PORT=5000
NODE_ENV=development

# DSPy Integration Configuration
ENABLE_DSPY_INTEGRATION=true
DSPY_SERVICE_URL=http://localhost:8001
DSPY_FALLBACK_TO_GEMINI=true
DSPY_REQUEST_TIMEOUT=30000

# File Upload Configuration
MAX_FILE_SIZE=20971520
MAX_FILES_PER_CHAT=3
UPLOAD_DIR=uploads

# Vector Memory Configuration
VECTOR_MEMORY_ENABLED=true
VECTOR_MEMORY_DIMENSIONS=768

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/backend.log

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Session Configuration
SESSION_SECRET=your_session_secret_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
