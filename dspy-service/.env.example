# API Keys (Only Google API Key needed)
GOOGLE_API_KEY=your_google_api_key_here
# OPENAI_API_KEY=not_required_for_gemini_only_setup
# ANTHROPIC_API_KEY=not_required_for_gemini_only_setup

# Service Configuration
SERVICE_HOST=0.0.0.0
SERVICE_PORT=8001
LOG_LEVEL=INFO

# Model Configuration (All using Google/Gemini)
DEFAULT_LM_MODEL=gemini/gemini-2.5-flash
EMBEDDING_MODEL=gemini-embedding-001
EMBEDDING_DIMENSIONS=768

# Vector Database
VECTOR_DB_TYPE=memory  # Options: memory, chroma, pinecone
CHROMA_HOST=localhost
CHROMA_PORT=8000
PINECONE_ENVIRONMENT=us-west1-gcp

# Optimization Settings
ENABLE_OPTIMIZATION=true
OPTIMIZATION_CACHE_DIR=./cache
MAX_OPTIMIZATION_EXAMPLES=500

# Performance Settings
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=30
CACHE_TTL=3600
