{
  "name": "ai-tutor-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "AI Tutor Application powered by Google Gemini 2.5 Pro",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite --host 0.0.0.0 --port 3000",
    "server:dev": "MONGO_URI=mongodb://localhost:27017/ai-tutor nodemon --no-warnings backend/server.js",
    "server": "MONGO_URI=mongodb://localhost:27017/ai-tutor node --no-warnings backend/server.js",
    "build": "vite build",
    "preview": "vite preview",
    "import-data": "MONGO_URI=mongodb://localhost:27017/ai-tutor node --no-warnings backend/scripts/importData.js",
    "export-data": "node --no-warnings backend/scripts/exportData.js",
    "setup-db": "node --no-warnings backend/scripts/setupDatabase.js"
  },
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@google/generative-ai": "^0.24.1",
    "@xenova/transformers": "^2.17.2",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "chromadb": "^3.0.9",
    "chromadb-default-embed": "^2.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "sharp": "^0.34.3",
    "socket.io": "^4.7.4",
    "tailwindcss": "^3.3.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "vite": "^5.0.0"
  }
}
